# talabalar = {
#     "Ali": {"math": 90, "english": 85, "science": 88},
#     "Vali": {"math": 92, "english": 80, "science": 86},
#     "Sami": {"math": 88, "english": 90, "science": 91},
#     "Karim": {"math": 92, "english": 90, "science": 87},
#     "Zafar": {"math": 92, "english": 90, "science": 87},  



student ={
"ism":"sardor",
"fanlar":{
"matematika":5,
"fizika": 3,
"kimyo":4,
"tarix":5
  }
}
for key, val in student ["fanlar"].items():
   print(f"{key} -{val}")
avg=sum(student["fanlar"].values ())/ len (student["fanlar"])
if avg>4.5:
   print("alochi:{avg}")
else:
   print("oddiy talaba:{avg}")